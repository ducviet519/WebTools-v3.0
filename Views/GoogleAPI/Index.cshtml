@model WebTools.Models.GoogleFilesViewModel
@{
    ViewData["Title"] = "Test Google API";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="dataTables_wrapper dt-bootstrap4">
    <div class="row">
        <div class="col-sm-12">
            <table class="table table-bordered table-striped dataTable dtr-inline" role="grid" id="tableReport">
                <thead class="thead">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Size</th>
                        <th>Version</th>
                        <th>CreatedTime</th>
                        <th>Parents</th>
                        <th>MimeType</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        if (Model.GoogleDriveList != null && Model.GoogleDriveList.Count > 0)
                        {
                            foreach (var item in Model.GoogleDriveList)
                            {
                                <tr>
                                    <td>@item.Id</td>
                                    <td>@item.Name</td>
                                    <td>@item.Size</td>
                                    <td>@item.Version</td>
                                    <td>@item.CreatedTime</td>
                                    <td>@item.Parents</td>
                                    <td>@item.MimeType</td>     
                                </tr>
                            }
                        }
                    }
                </tbody>               
            </table>
        </div>
    </div>
</div>